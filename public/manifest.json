{
  "manifest_version": 3,
  "name": "Website Pintabs Organizer by Ntamas",
  "version": "1.1.2",
  "description": "Save and organize websites into categories and open them in pinned tabs.",
  "permissions": ["storage"],
  "icons": {
    "16": "logo/WPO-Logo-16.png",
    "48": "logo/WPO-Logo-48.png",
    "128": "logo/WPO-Logo-128.png"
  },
  "background": {
    "service_worker": "assets/background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo/WPO-Logo-128.png"
  },
  "host_permissions": ["<all_urls>"]
}
